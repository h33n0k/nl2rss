	"main": "dist/index.js",
	"bin": "dist/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=production LOG_LEVEL=info node dist/src/index.js",
		"dev": "cross-env NODE_ENV=development LOG_LEVEL=debug ts-node-dev --quiet --clear --rs --transpile-only src/index.ts",
		"build": "cross-env NODE_ENV=production npx tsc -p ./tsconfig.build.json && cp -r src/views dist/src/ && cp -r assets/ dist/",
		"clean": "rm -rf dist",
		"lint": "eslint -c ./.eslintrc.cjs \"./**/*.{js,ts}\" --fix",
		"test": "cross-env NODE_ENV=development LOG_LEVEL=verbose jest"
	},
