{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
    "emitDecoratorMetadata": true,
		"forceConsistentCasingInFileNames": true,
		"strictPropertyInitialization": false,
		"skipLibCheck": true,
		"lib": ["es2018", "esnext.asynciterable"],
		"baseUrl": "src"
	}
}
